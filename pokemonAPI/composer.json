{
  "name": "graphicresources/dev-env-boilerplate",
  "description": "Ejemplo b√°sico de proyecto que hace uso del dev-env-boilerplate",
  "require": {
    "php": "^8",
    "monolog/monolog": "^2",
    "ext-pdo_mysql": "*",
    "slim/slim": "^4",
    "slim/psr7": "^1",
    "php-di/php-di": "^6",
    "predis/predis": "v0.8.7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9",
    "symfony/var-dumper": "^5.0",
    "squizlabs/php_codesniffer": "^3",
    "phpstan/phpstan": "^1",
    "codedungeon/phpunit-result-printer": "^0"
  },
  "autoload": {
    "psr-4": {
      "GR\\DevEnvBoilerplate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GR\\DevEnvBoilerplate\\Tests\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
